<%= form_for(:user,url: users_path,:html => {:align => "center",
	:id => "contactForm"},remote: true) do |form|%>
	<p>
		<%= form.label :name %>
		<%= form.text_field :name %>
	</p>
	<p>
		<%= form.label :email %>
		<%= form.text_field :email %>
	</p>
	<p>
		<%= form.submit("Add User") %>
	</p>
<% end %>